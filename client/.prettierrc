{
  "singleQuote": false,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 80,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "App",
    "AppInitializer",
    "AppRoutes",
    "mockStore",
    "serviceWorker",
    "V2PrivateRoutes",
    "theme",
    "reducers",
    "^assets/(.*)$",
    "^components/(.*)$",
    "^core/(.*)$",
    "^hooks/(.*)$",
    "^layouts/(.*)$",
    "^locales/(.*)$",
    "^router/(.*)$",
    "^services/(.*)$",
    "^store/(.*)$",
    "^theme/(.*)$",
    "^views/(.*)$",
    "^utils/(.*)$",
    ".scss$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
